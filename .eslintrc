{
  "env": {
    "browser": false,
    "es6": true,
    "node": true
  },
  "extends": ["eslint:recommended"],
  "globals": {
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true
    }
  },
  "plugins": [
  ],
  "rules": {
    "indent": ["error", 2, { "SwitchCase": 1 }],
    "linebreak-style": ["error", "unix"],
    "no-unsafe-finally": [0],
    "no-var": 2,
    "quotes": ["error", "single"],
    "semi": ["error", "never"],
    "strict": ["error", "never"]
  }
}
